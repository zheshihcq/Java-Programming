public class ContinueKeywordExample5_11 {
    public static void main(String[] args) {
        for (int i = 1; i <= 10; i++) {
            if (i % 2 == 0) {
                continue; // 当i为偶数时，跳过本次循环的剩余部分，直接进行下一次循环
            }
            System.out.print(i + " ");
        }
        System.out.println();
    }
}
 
